class A{
	Public void method(I i) {
		b.method();
	}
}
//B클래스의 선언과 구현을 분리
interface I {
	Public void method();
}
class B implements I {
	Public void method() {
		System.out.println(“B클래스의 메서드”);
	}
}

class C {
	Public void method() {
		System.out.println(“c클래스의 메서드”);
	}
}

public class interfaceTest {
	Public static void main(String[] args) {
		A a = new A();
		A.method(new c());
	}
}

